	<!-- start: Favicon -->
	<link rel="shortcut icon" href="img/1.png">
	<!-- end: Favicon -->



		<!-- start: Header -->
	<div class="navbar">
		<div class="navbar-inner">
			<div class="container-fluid">
				<a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</a>		<a class="brand" > <img alt="Administación de Unidad" src="img/1.png" /> <span>Administación de Unidad Habitacional</span></a>

				<!-- start: Header Menu -->
			<div class="btn-group pull-right" >
				{{#link-to "alertas"}}

					<a class="btn" href="alertas.html">
						<i class="icon-warning-sign"></i><span class="hidden-phone hidden-tablet"> Alertas</span>
						<span class="label label-important hidden-phone">
							{{#if currentUnit}}
								{{currentUnit.alertas.length}}
							{{else}}
								0
							{{/if}}
						</span> <span class="label label-success hidden-phone"></span>
					</a>
					{{/link-to}}
					{{#link-to "encuestas"}}
					<a class="btn">
						<i class="icon-tasks"></i><span class="hidden-phone hidden-tablet"> Encuestas</span>
						<span class="label label-warning hidden-phone">
							{{#if currentUnit}}
								{{currentUnit.encuestas.length}}
							{{else}}
								0
							{{/if}}
						</span>
					</a>
					{{/link-to}}
					{{#link-to "solicitudes"}}
					<a class="btn">
						<i class="icon-envelope"></i><span class="hidden-phone hidden-tablet"> Solicitudes</span>
						<span class="label label-success hidden-phone">
							{{#if currentUnit}}
								{{currentUnit.solicitudes.length}}
							{{else}}
								0
							{{/if}}
						</span>
					</a>
					{{/link-to}}
					<!-- start: User Dropdown -->
					<a class="btn dropdown-toggle" data-toggle="dropdown">
						<i class="icon-user"></i><span class="hidden-phone hidden-tablet"> Perfil</span>
						<span class="caret"></span>
					</a>
					<ul class="dropdown-menu">
						<li><a>Ajustes</a></li>
						<li class="divider"></li>
						<li><a {{action 'signOut'}}>Cerrar Sesión</a></li>
					</ul>
					<!-- end: User Dropdown -->
				</div>
				<!-- end: Header Menu -->

			</div>
		</div>
	</div>


		<div class="container-fluid">
		<div class="row-fluid">

			<!-- start: Main Menu -->
		<!-- start: Main Menu -->
			<div class="span2 main-menu-span">
